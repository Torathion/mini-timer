{
  "name": "micro-timer",
  "version": "1.0.0",
  "description": "The tiniest timer to tick!",
  "types": "./index.d.ts",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "exports": {
    ".": {
      "types": "./index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "scripts": {
    "build": "node esbuild.config.js",
    "lint": "eslint src --fix",
    "format": "npx prettier --write */**.ts",
    "test": "vitest"
  },
  "keywords": [
    "small",
    "timer",
    "micro",
    "tiny",
    "utility"
  ],
  "author": "Torathion",
  "homepage": "https://github.com/Torathion/node-streamweaver",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Torathion/node-streamweaver.git"
  },
  "bugs": {
    "url": "https://github.com/Torathion/node-streamweaver/issues"
  },
  "license": "MIT",
  "type": "module",
  "sideEffects": false,
  "packageManager": "pnpm@10.11.0",
  "devDependencies": {
    "@rollup/rollup-win32-ia32-msvc": "^4.41.0",
    "@size-limit/file": "^11.2.0",
    "@size-limit/preset-app": "^11.2.0",
    "@vitest/coverage-v8": "^3.1.4",
    "esbuild": "^0.25.4",
    "eslint": "^9.27.0",
    "eslint-config-shiny": "^4.2.1",
    "jiti": "^2.4.2",
    "size-limit": "^11.2.0",
    "typescript": "^5.8.3",
    "typestar": "^1.11.0",
    "vite": "^6.3.5",
    "vitest": "^3.1.4"
  },
  "dependencies": {
    "mitt": "^3.0.1"
  }
}
